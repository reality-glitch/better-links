{
  "name": "bye bye wechat",
  "version": "0.2.2",
  "description": "帮助去掉电脑版微信对某些网址的屏蔽",
  "author": "Ya Zhuang <zhuangya@gmail.com>",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://weixin110.qq.com/*",
    "*://support.weixin.qq.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  }
}
